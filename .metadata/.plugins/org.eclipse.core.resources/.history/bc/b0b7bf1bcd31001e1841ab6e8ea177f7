$(function(){
	$("a").eq(0).on("click", function(){
		$(location).attr("href","./download.do?seq="+$("input[type=hidden]").eq(0).val());
	});
	
	$("li a").on("click", function(){
		console.log($(this).attr("id"));
	});
});

function changePage(){
	$("input[type=hidden]").eq(0).val()
	$(location).attr("href","./detail.do?seq=1&page=1");
}