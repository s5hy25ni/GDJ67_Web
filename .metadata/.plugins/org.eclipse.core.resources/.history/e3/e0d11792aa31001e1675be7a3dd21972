$(function(){
	
});

function page(start, end){
	$.ajax({
		url:"./reply.do",
		type:"post",
		async: true,
		dataType:"json",
		data:{"ref":$("input[type=hidden]").val(),
				"start":start,
				"end":end},
		success:function(resp){
			console.log(resp)
		},
		error:function(){
			alert("잘못된 요청");
		}
	});
}